    "react-gesture-gallery": "^1.0.1",
    "react-gesture-responder": "^2.1.0",
    "react-moment": "^0.9.2",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "styled-components": "^4.3.2"
